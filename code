local downloaded = 0
local skipped = 0
local assetsdownload = 0
local downloadfile = function(meta)
local fil = "UhhhhhhReanim/Assets/" .. meta.name
if not redownloadeverything then
local s, d = pcall(isfile, fil)
if s and d then
skipped += 1
return
end
end
local s, body = pcall(request, {
Url = "https://raw.githubusercontent.com/STEVE-916-create/Uhhhhhh/main/assets/" .. meta.name,
Method = "GET"
})
if s and body then
pcall(writefile, fil, body.Body)
downloaded += 1
end
end

